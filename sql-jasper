select 
sum (ORDERFACT .$SP !{param_indicateur}) as INDICATEUR, CUSTUMER.COUNTRY AS PAYS, PRDUCTS.PRODUCTLINE AS GAME_PRODUIT,
ORDERFACT.YEAR_ID  AS ANNE FROM PRDUCTS 
INNER JOIN ORDERFACT ON PRODUCTS.PRODUCTCODE = ORDERFACT.PRODUCTCODE
INNER JOIN CUSTOMER ON ORDERFACT.CUTUMERNUMBER = CUMSTUMER.CUSTUMERNUMBER
WHERE CUSTUMER.COUNTRY = $P{PARAM_PAYS}
GROUP BY PAYS , AMEPRODCUIT , ANNEE
